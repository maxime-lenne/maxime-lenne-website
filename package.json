{
  "name": "maxime-lenne-website",
  "version": "1.0.0",
  "description": "Static portfolio website for Maxime Lenne built with Jekyll SSG and JAMstack architecture",
  "main": "index.js",
  "scripts": {
    "build": "jekyll build",
    "serve": "jekyll serve --livereload",
    "build:prod": "JEKYLL_ENV=production jekyll build",
    "optimize:images": "imagemin assets/images/* --out-dir=assets/images/optimized",
    "minify:assets": "uglifyjs assets/js/*.js -o assets/js/min/ && cleancss -o assets/css/min/ assets/css/*.css",
    "generate:favicons": "pwa-asset-generator assets/images/logo.png",
    "test:lighthouse": "lighthouse http://localhost:4000 --output=html --output-path=./lighthouse-report.html",
    "test:accessibility": "axe http://localhost:4000",
    "lint:css": "stylelint assets/css/**/*.css",
    "lint:js": "eslint assets/js/**/*.js"
  },
  "keywords": [
    "jekyll",
    "portfolio",
    "jamstack",
    "notion",
    "github-pages"
  ],
  "author": "Maxime Lenne",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.0",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "autoprefixer": "^10.4.20",
    "cleancss-cli": "^5.6.2",
    "eslint": "^8.57.0",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "lighthouse": "^12.0.0",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "pwa-asset-generator": "^6.0.0",
    "sass": "^1.72.0",
    "stylelint": "^17.1.0",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "axe-core": "^4.10.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.0.0"
  }
}
